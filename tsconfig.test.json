{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "out-tests",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strict": false,
    "noImplicitAny": false
  },
  "include": [
    "src/**/*",
    "tests/**/*"
  ],
  "exclude": [
    "node_modules",
    "out"
  ]
}
